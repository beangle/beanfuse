<assembly>
  <id>dddd</id>
  <formats>
    <format>jar</format>
  </formats>
  <fileSets>
    <fileSet>
      <includes>
        <include>pom.xml</include>
      </includes>
      <useDefaultExcludes>true</useDefaultExcludes>
    </fileSet>
    <fileSet>
      <directory>src</directory>
      <useDefaultExcludes>true</useDefaultExcludes>
    </fileSet>
  </fileSets>
  <moduleSets>
    <moduleSet>
      <includes>
        <include>org.beanfuse:model</include>
      </includes>
      <binaries>
        <outputDirectory>${artifactId}/target/classes</outputDirectory>
      </binaries>
    </moduleSet>
  </moduleSets>

</assembly>
